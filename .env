# URL สำหรับเชื่อมต่อกับ Backend (Node.js)
VITE_API_BASE_URL=http://project.3bbddns.com:36145

# Dev server host/port for Vite
VITE_DEV_HOST=project.3bbddns.com
VITE_DEV_PORT=36144

# App Version
VITE_APP_VERSION=v3.0

# Bot Sleep Timeout (milliseconds) - how long before bot falls asleep when idle
VITE_BOT_SLEEP_TIMEOUT=20000

# Snow Configuration
VITE_SNOW_COUNT=20
VITE_SNOW_MIN_SIZE=0.4
VITE_SNOW_MAX_SIZE=2.4
VITE_SNOW_MIN_DURATION=10
VITE_SNOW_MAX_DURATION=25
VITE_SNOW_OPACITY=0.7


VITE_BOT_TYPING_SPEED=5

# Auto open chatbot on load (true = เปิดอัตโนมัติ, false = ปิด)
VITE_AUTO_OPEN_CHATBOT=true

# Z-index for snowflakes effect (default: 1000)
VITE_SNOW_Z_INDEX=1000
